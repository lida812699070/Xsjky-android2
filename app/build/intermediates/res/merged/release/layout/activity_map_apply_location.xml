<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.xsjky.android.ui.MapApplyLocation">

    <com.baidu.mapapi.map.MapView
        android:id="@+id/bmapView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" />
    <Button
        android:id="@+id/map_open_LBS"
        android:text="开启导航"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <Button
        android:text="导航列表"
        android:layout_marginLeft="20dp"
        android:layout_toRightOf="@id/map_open_LBS"
        android:id="@+id/btn_show_LBSlv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <ListView
        android:listSelector="#500f"
        android:layout_alignLeft="@id/btn_show_LBSlv"
        android:layout_below="@id/btn_show_LBSlv"
        android:id="@+id/lv_LBS_locations"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"></ListView>

</RelativeLayout>
